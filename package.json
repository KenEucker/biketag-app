{
  "name": "biketag-app",
  "version": "2.0.0",
  "description": "The web application for BikeTag Games",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keneucker/biketag-app.git"
  },
  "author": "Ken Eucker",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/keneucker/biketag-app/issues"
  },
  "engines": {
    "node": ">=16"
  },
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/biketag"
  },
  "homepage": "https://keneucker.github.io/biketag-app/",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf lib dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier . --check ",
    "lint": "npm run format:check && npm run lint:check && npm run typecheck && echo 'ðŸ¤– !linter âœ… success! ðŸ¤–'",
    "lint:check": "eslint .",
    "lint:write": "prettier . --write && eslint . --fix",
    "lint:staged": "npx --no-install lint-staged",
    "commit": "cz",
    "convert": "tsoa spec-and-routes",
    "dev": "netlify dev --port 8080 --functionsPort 7200",
    "build": "vite build",
    "preview": "vite build && vite preview",
    "serve": "vite",
    "test": "cypress open",
    "test-components": "cypress open-ct",
    "test:ci": "cypress run --headless",
    "test:ci-components": "cypress run-ct"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.3.5",
    "@vueuse/head": "~0.7",
    "@fawmi/vue-google-maps": "^0.9.4",
    "biketag": "^2.1.4",
    "node-fetch": "^3.1.0",
    "papaparse": "^5.3.1",
    "vue": "~3.2",
    "vue-router": "~4.0",
    "vuex": "~4.0",
    "@ionic/vue": "^6.0.13",
    "pinia": "^2.0.12",
    "tailwindcss": "^3.0.23",
    "@auth0/auth0-spa-js": "^1.19.4",
    "vue-toast-notification": "^3.0.0",
    "jose": "^4.6.0",
    "ajv": "^8.10.0",
    "crypto-js": "^4.1.1",
    "lzutf8": "^0.6.1",
    "@serverless-jwt/jwt-verifier": "^0.2.1"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "~2.2",
    "@cypress/vue": "~3.0",
    "@netlify/functions": "^0.11.0",
    "@tailwindcss/aspect-ratio": "~0.4",
    "@tailwindcss/forms": "~0.4",
    "@tailwindcss/line-clamp": "~0.3",
    "@tailwindcss/typography": "~0.5",
    "@types/node": "^17.0.8",
    "@types/papaparse": "^5.3.1",
    "@types/request": "^2.48.8",
    "@typescript-eslint/eslint-plugin": "~5",
    "@typescript-eslint/parser": "~5",
    "@vitejs/plugin-vue": "~2",
    "@vue/eslint-config-standard": "~6.1",
    "@vue/composition-api": "^1.4.4",
    "@vue/eslint-config-typescript": "~10",
    "autoprefixer": "~10",
    "cypress": "~9.3",
    "dotenv": "^10.0.0",
    "eslint": "~8.7",
    "eslint-config-prettier": "~8.3",
    "eslint-plugin-import": "~2",
    "eslint-plugin-node": "~11.1",
    "eslint-plugin-promise": "~6.0",
    "eslint-plugin-vue": "~8",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.4",
    "netlify-cli": "^8.14.1",
    "postcss": "~8.4",
    "postcss-import": "^14.0.2",
    "postcss-nesting": "~10",
    "prettier": "^2.5.1",
    "semantic-release": "^18.0.1",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "sass": "^1.45.2",
    "tsoa": "^3.14.1",
    "typescript": "~4.5",
    "vite": "^2.9.5",
    "vite-plugin-env-compatible": "^1.1.1",
    "vite-plugin-environment": "^1.1.1",
    "wait-on": "~6.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint . --fix",
      "jest --bail --findRelatedTests"
    ],
    "*.{js,css,md,yml,yaml,json}": "prettier . --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "keywords": [
    "typescript",
    "biketag"
  ]
}
