{
  "name": "biketag-app",
  "version": "0.0.1",
  "description": "The web application for BikeTag Games",
  "main": "./biketag-app.node.js",
  "browser": "./biketag-app.js",
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keneucker/biketag-app.git"
  },
  "author": "Ken Eucker",
  "license": "CC-BY-SA-4.0",
  "bugs": {
    "url": "https://github.com/keneucker/biketag-app/issues"
  },
  "engines": {
    "node": ">=16"
  },
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/biketag"
  },
  "homepage": "https://keneucker.github.io/biketag-app/",
  "scripts": {
    "test": "jest --verbose",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "build": "tsc",
    "expose": "cp -v dist/* ./",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf lib dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier . --check ",
    "lint": "npm run format:check && npm run lint:check && npm run typecheck && echo 'ðŸ¤– !linter âœ… success! ðŸ¤–'",
    "lint:check": "eslint .",
    "lint:write": "prettier . --write && eslint . --fix",
    "lint:staged": "npx --no-install lint-staged",
    "commit": "cz",
    "convert": "tsoa spec-and-routes"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.3.5",
    "biketag": "^1.14.2",
    "node-fetch": "^3.1.0",
    "papaparse": "^5.3.1"
  },
  "devDependencies": {
    "@netlify/functions": "^0.10.0",
    "@types/node": "^17.0.8",
    "@types/papaparse": "^5.3.1",
    "dotenv": "^10.0.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.4",
    "prettier": "^2.5.1",
    "semantic-release": "^18.0.1",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsoa": "^3.14.1",
    "typescript": "^4.5.4"
  },
  "lint-staged": {
    "*.ts": [
      "eslint . --fix",
      "jest --bail --findRelatedTests"
    ],
    "*.{js,css,md,yml,yaml,json}": "prettier . --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "keywords": [
    "typescript",
    "biketag"
  ]
}
