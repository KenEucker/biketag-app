{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "biketag-app",
  "description": "The administration application for BikeTag Games.",
  "version": "1.0.11",
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Ken Eucker",
    "email": "keneucker@gmail.com",
    "url": "https://keneucker.com/"
  },
  "homepage": "https://github.com/keneucker/biketag-app",
  "repository": {
    "type": "git",
    "url": "git@github.com:keneucker/biketag-app.git"
  },
  "bugs": {
    "url": "https://github.com/keneucker/biketag-app/issues"
  },
  "engines": {
    "node": ">=18.12.1",
    "yarn": ">=1.22.19"
  },
  "packageManager": "yarn@3.3.1",
  "scripts": {
    "dev": "netlify dev --targetPort 8080 --functionsPort 7200",
    "clean": "rimraf ./node_modules/.vite",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "pnpm run lint:js && pnpm run lint:style",
    "lint:js": "eslint . --fix --ext .ts,.js,.vue --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . -w -u",
    "lint:style": "stylelint \"./src/**/*.{css,sass,scss,htm,html,vue}\" --fix --cache-location ./node_modules/.vite/vite-plugin-stylelint  && prettier  \"./src/**/*.{css,sass,scss,htm,html,vue}\" -w -u",
    "start": "vite",
    "test": "vitest",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:coverage": "vitest run --coverage",
    "build": "run-p type-check build-only",
    "build:analyze": "vite build --mode analyze",
    "build:clean": "rimraf dist",
    "build-only": "vite build",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@auth0/auth0-vue": "^1.0.2",
    "@mdi/font": "^7.1.96",
    "@vueuse/head": "^1.0.22",
    "ajv": "^8.12.0",
    "axios": "^1.2.2",
    "biketag": "^2.1.4",
    "core-js": "^3.27.1",
    "jszip": "^3.10.1",
    "pinia": "^2.0.28",
    "pinia-plugin-persistedstate": "^3.0.2",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vuetify": "^3.1.0",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@netlify/functions": "^1.4.0",
    "@serverless-jwt/jwt-verifier": "^0.2.1",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.18",
    "@types/webfontloader": "^1.6.35",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.26.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/core": "^9.10.0",
    "crypto-js": "^4.1.1",
    "eslint": "^8.31.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.6.4",
    "eslint-plugin-jsonc": "^2.5.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-vue": "^9.8.0",
    "eslint-plugin-vuejs-accessibility": "^2.0.0",
    "eslint-plugin-vuetify": "^2.0.0-beta.2",
    "eslint-plugin-yaml": "^0.5.0",
    "husky": "^8.0.2",
    "jose": "^4.11.2",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.1.0",
    "lzutf8": "^0.6.3",
    "netlify-cli": "^12.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.1",
    "qs": "^6.11.0",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.57.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-order": "^5.0.0",
    "stylelint-selector-bem-pattern": "^2.1.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vite-plugin-checker": "0.5.3",
    "vite-plugin-vue-markdown": "^0.22.2",
    "vite-plugin-vuetify": "^1.0.1",
    "vitest": "^0.26.3",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.0.19"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,jsonc,json5,yaml,yml,toml,yml,yaml,vue,htm,html,md}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*.{css,sass,scss,vue,htm,html}": "stylelint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-stylelint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "json5": "^2.2.3",
    "prettier": "^2.8.1"
  },
  "stackblitz": {
    "startCommand": "yarn run test"
  }
}
